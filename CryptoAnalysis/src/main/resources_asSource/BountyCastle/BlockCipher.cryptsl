SPEC org.bouncycastle.crypto.BlockCipher

OBJECTS
	org.bouncycastle.crypto.CipherParameters params;
	byte[] buf;
	
EVENTS	
	i : init(_, params);
	
	p : processBlock(buf, _, buf, _);
	
ORDER
	i, p*
	
REQUIRES
	generatedEngineOrMode[this];
	generatedCipherParams[params];
	
ENSURES
	encrypted[buf] after p;