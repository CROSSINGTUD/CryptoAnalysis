SPEC javax.crypto.spec.PBEParameterSpec
OBJECTS 
	byte[] salt;
	int iterationCount;
	java.security.spec.AlgorithmParameterSpec paramSpec;
	
EVENTS
	c1: PBEParameterSpec(salt, iterationCount);
	c2: PBEParameterSpec(salt, iterationCount, paramSpec);
	Cons := c1 | c2;
	
ORDER
	Cons
CONSTRAINTS
	iterationCount >= 10000;	

REQUIRES
	randomized[salt];

ENSURES
	preparedPBE[this];